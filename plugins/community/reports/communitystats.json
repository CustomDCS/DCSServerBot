{
  "color": "blue",
  "title": "DCSServerBot Community Overview",
  "elements": [
    {
      "class": "plugins.community.report.CommunityStats"
    },
    {
      "type": "Ruler",
      "params": {
        "ruler_length": 20
      }
    },
    {
      "type": "Graph",
      "params": {
        "width": 15,
        "height": 15,
        "rows": 3,
        "cols": 2,
        "elements":
         [
            {
              "class": "plugins.community.report.BotVersions",
              "params": { "row": 0, "col": 0 }
            },
            {
              "class": "plugins.community.report.DCSVersions",
              "params": { "row": 0, "col": 1 }
            },
            {
              "class": "plugins.community.report.PythonVersions",
              "params": { "row": 1, "col": 0 }
            },
            {
              "class": "plugins.community.report.BotVariants",
              "params": { "row": 1, "col": 1 }
            },
            {
              "class": "plugins.community.report.Plugins",
              "params": { "row": 2, "col": 0, "colspan": 2 }
            }
         ]
      }
    }
  ],
  "footer": "Only showing servers that sent registration information\nand that were online in the past 7 days."
}